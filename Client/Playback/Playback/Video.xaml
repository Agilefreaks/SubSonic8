<Page
    x:Class="Subsonic8.Playback.Playback.Video"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:videoPlayback="using:Subsonic8.VideoPlayback" xmlns:cal="using:Caliburn.Micro"
    mc:Ignorable="d">

    <ContentControl HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch">
        <videoPlayback:EmbeddedVideoPlaybackView cal:Bind.Model="{Binding Path=EmbeddedVideoPlaybackViewModel}" />
    </ContentControl>
</Page>